                    "for": {
                        "in": "account"
                    },
                    "dataReductionAlgorithm": {
                        "window": {
                            "count": 20000
                        }
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "date"
                            }
                        },
                        {
                            "bind": {
                                "to": "values"
                            }
                        }
                    ],
                    "dataReductionAlgorithm": {
                        "window": {
                            "count": 20000
                        }
                    }
                }
            }
        }
    ],
