{
    "dataRoles": [
        {
            "displayName": "Accounts",
            "name": "rows",
            "kind": "Grouping"
        },
        {
            "displayName": "Value columns",
            "name": "columns",
            "kind": "Measure"
        },
        {
            "displayName": "Date Accounts",
            "name": "date",
            "kind": "Measure"
        },
        {
            "displayName": "Sort accounts",
            "name": "sort_accounts",
            "kind": "Measure"
        }
    ],
    "objects": {
        "dataPoint": {
            "properties": {
                "defaultColor": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "showAllDataPoints": {
                    "type": {
                        "bool": true
                    }
                },
                "fill": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "fillRule": {
                    "type": {
                        "fill": {}
                    }
                },
                "fontSize": {
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "rows": {
                        "max": 5
                    },
                    "columns": {
                        "max": 6
                    },
                    "sort_accounts": {
                        "max": 1
                    },
                    "date": {
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "rows"
                    },
                    "sort_accounts": {
                        "top": {}
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "columns"
                            }
                        },
                        {
                            "bind": {
                                "to": "sort_accounts"
                            }
                        },
                        {
                            "bind": {
                                "to": "date"
                            }
                        }
                    ]
                }
            }
        }
    ],
    "sorting": {
        "implicit": {
            "clauses": [
                {
                    "role": "sort_accounts",
                    "direction": 1
                }
            ]
        }
    },
    "privileges": [
        {
            "name": "ExportContent",
            "essential": true
        }
    ]
}